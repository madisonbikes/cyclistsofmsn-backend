{
  "name": "cyclistsofmsn-backend",
  "version": "2.1.0",
  "description": "Cyclists of Madison Backend",
  "private": true,
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "start": "PINO_PRETTY=true vite-node src/main.ts",
    "compile": "tsc --noEmit",
    "dist": "tsc && npm run genversion",
    "dev": "PINO_PRETTY=true vite-node --watch src/main.ts",
    "pm2-start": "pm2 startOrRestart ecosystem.config.js --update-env",
    "pm2-stop": "pm2 stop ecosystem.config.js",
    "test": "PINO_PRETTY=true vitest run",
    "test-watch": "PINO_PRETTY=true vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --max-warnings=0",
    "register_twitter": "vite-node src/twitter/register_main.ts",
    "test_tweet": "vite-node src/twitter/main.ts 'this is a test status' test_resources/test_DSC_7020.jpg",
    "test_toot": "vite-node src/mastodon/main.ts 'this is a test status' test_resources/test_DSC_7020.jpg",
    "test_bsky": "vite-node src/atproto/main.ts 'this is a test status' test_resources/test_DSC_7020.jpg",
    "check-package-updates": "npm-check-updates -x p-limit -x tempfile",
    "prepare": "husky",
    "genversion": "genversion -f -u dist/src/backend-version.js",
    "knip": "knip",
    "check": "npm run compile && npm run test && npm run lint &&  npm run knip"
  },
  "author": "Ben Sandee",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=22.x"
  },
  "dependencies": {
    "@atproto/api": "^0.15.12",
    "@keyv/valkey": "^1.0.6",
    "bcryptjs": "^3.0.2",
    "connect-redis": "^8.1.0",
    "connection-string": "^4.4.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.0",
    "exifreader": "^4.31.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "fs-extra": "^11.3.0",
    "iovalkey": "^0.3.2",
    "keyv": "^5.3.3",
    "mongodb": "^6.17.0",
    "oauth-1.0a": "^2.2.6",
    "p-limit": "~3.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pino": "^9.7.0",
    "promisify-child-process": "^4.1.2",
    "readline-sync": "^1.4.10",
    "sharp": "0.32.6",
    "superagent": "^10.2.1",
    "tempfile": "^3.0.0",
    "twitter-api-client": "^1.6.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.25.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/cors": "^2.8.19",
    "@types/express-session": "^1.18.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.29",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/readline-sync": "^1.4.8",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-istanbul": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-promise": "^7.2.1",
    "genversion": "^3.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "knip": "^5.61.3",
    "mongodb-memory-server": "^10.1.4",
    "npm-check-updates": "^18.0.1",
    "pino-pretty": "^13.0.0",
    "pm2": "^6.0.6",
    "prettier": "3.6.2",
    "supertest": "^7.1.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite-node": "^3.2.3",
    "vitest": "^3.2.3"
  }
}