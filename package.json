{
  "name": "cyclistsofmsn-backend",
  "version": "2.1.0",
  "description": "Cyclists of Madison Backend",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts | pino-pretty -c",
    "compile": "tsc --noEmit",
    "dist": "tsc && npm run genversion",
    "dev": "ts-node-dev --inspect -- src/server.ts | pino-pretty -c",
    "migration:imagedescription": "ts-node src/tools/updatefilemetadata.ts",
    "pm2-start": "pm2 startOrRestart ecosystem.config.js --update-env",
    "pm2-stop": "pm2 stop ecosystem.config.js",
    "test": "NODE_ENV=test jest --maxWorkers=50%",
    "test-watch": "NODE_ENV=test jest --maxWorkers=25% --watch",
    "coverage": "NODE_ENV=test jest --maxWorkers=50% --coverage",
    "lint": "eslint . --max-warnings=0",
    "register_twitter": "ts-node src/twitter/register.ts",
    "test_tweet": "ts-node src/twitter/post.ts 'this is a test status' test_resources/test_DSC_7020.jpg",
    "test_toot": "ts-node src/mastodon/post.ts 'this is a test status' test_resources/test_DSC_7020.jpg",
    "test_bsky": "ts-node src/bsky/index.ts 'this is a test status' test_resources/test_DSC_7020.jpg",
    "check-package-updates": "npm-check-updates -x p-limit -x tempfile",
    "prepare": "husky",
    "genversion": "genversion -f -u dist/src/backend-version.js",
    "knip": "knip",
    "check": "npm run compile && npm run lint && npm run knip && npm run test"
  },
  "author": "Ben Sandee",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.x"
  },
  "dependencies": {
    "@atproto/api": "^0.15.12",
    "@keyv/valkey": "^1.0.3",
    "@typegoose/typegoose": "^12.16.0",
    "bcryptjs": "^3.0.2",
    "connect-redis": "^8.1.0",
    "connection-string": "^4.4.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "exifreader": "^4.31.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "fs-extra": "^11.3.0",
    "iovalkey": "^0.3.2",
    "keyv": "^5.3.3",
    "mongodb": "^6.16.0",
    "mongoose": "^8.9.6",
    "oauth-1.0a": "^2.2.6",
    "p-limit": "~3.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pino": "^9.6.0",
    "promisify-child-process": "^4.1.2",
    "readline-sync": "^1.4.10",
    "sharp": "0.32.6",
    "superagent": "^10.2.1",
    "tempfile": "^3.0.0",
    "twitter-api-client": "^1.6.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.25.1",
    "@tsconfig/node20": "^20.1.5",
    "@types/cors": "^2.8.18",
    "@types/express-session": "^1.18.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/readline-sync": "^1.4.8",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "genversion": "^3.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "knip": "^5.59.1",
    "mongodb-memory-server": "^10.1.4",
    "npm-check-updates": "^18.0.1",
    "pino-pretty": "^13.0.0",
    "pm2": "^6.0.5",
    "prettier": "3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  }
}